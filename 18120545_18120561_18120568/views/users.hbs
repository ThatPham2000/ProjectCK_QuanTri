<div class="content">

	<div class="module">
		<div class="module-head">
			<h3>List of Users</h3>
		</div>
		<div class="module-body">
			<table class="table table-striped">
				<thead>
					<tr>
						<th>#</th>
						<th>Name</th>
						<th>Email</th>
						<th>Phone</th>
						<th>Role</th>
					</tr>
				</thead>
				<tbody>
					{{#each users}}
					<tr>
						<td>{{@index}}</td>
						<td>
							<a href="#">{{name}}</a>
						</td>
						<td>{{email}}</td>
						<td>{{phone}}</td>
						<td>{{roles}}</td>
					</tr>
					{{/each}}
				</tbody>
			</table>
		</div>
	</div>
</div>
<!--/.content-->
